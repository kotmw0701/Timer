<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.Pane?>

<Pane onMouseDragged="#onDragg" onMousePressed="#onPress" prefHeight="100.0" prefWidth="600.0" style="-fx-border-style: solid; -fx-background-color: rgba(255,255,255,0.1);" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jp.motlof.timer.TimerController">
   <children>
      <Label fx:id="number" layoutX="32.0" layoutY="10.0" text="1" textFill="#b5b5b5">
         <font>
            <Font name="Meiryo Bold" size="50.0" />
         </font>
      </Label>
      <Label fx:id="timertext" alignment="CENTER" layoutX="107.0" layoutY="15.0" prefHeight="70.0" prefWidth="261.0" text="00:00:00.000" textAlignment="CENTER" textFill="#b9b9b9">
         <font>
            <Font name="Meiryo Bold" size="36.0" />
         </font>
      </Label>
      <Button layoutX="404.0" layoutY="58.0" mnemonicParsing="false" onAction="#onStop" prefHeight="25.0" prefWidth="72.0" text="停止">
         <font>
            <Font name="Meiryo" size="12.0" />
         </font></Button>
      <Button fx:id="pauseButton" layoutX="404.0" layoutY="14.0" mnemonicParsing="false" onAction="#onPause" prefHeight="29.0" prefWidth="72.0" text="一時停止">
         <font>
            <Font name="Meiryo" size="12.0" />
         </font></Button>
      <Button fx:id="restartButton" disable="true" layoutX="499.0" layoutY="14.0" mnemonicParsing="false" onAction="#onRestart" prefHeight="29.0" prefWidth="72.0" text="再起動">
         <font>
            <Font name="Meiryo" size="12.0" />
         </font></Button>
      <Button layoutX="499.0" layoutY="58.0" mnemonicParsing="false" onAction="#onDelete" prefHeight="29.0" prefWidth="72.0" text="消去">
         <font>
            <Font name="Meiryo" size="12.0" />
         </font></Button>
      <Separator layoutX="94.0" layoutY="3.0" orientation="VERTICAL" prefHeight="94.0" prefWidth="2.0" />
      <Separator layoutX="379.0" layoutY="3.0" orientation="VERTICAL" prefHeight="94.0" prefWidth="2.0" />
      <Button layoutX="578.0" layoutY="76.0" mnemonicParsing="false" onAction="#onOpenWindow" prefHeight="18.0" prefWidth="17.0" text="&gt;&gt;">
         <font>
            <Font size="6.0" />
         </font></Button>
      <Label fx:id="period" layoutX="248.0" layoutY="71.0" prefHeight="29.0" prefWidth="131.0" textFill="#b9b9b9">
         <font>
            <Font name="Meiryo Bold" size="14.0" />
         </font>
      </Label>
   </children>
</Pane>
